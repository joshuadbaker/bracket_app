
<div class="container-fluid">
  <div class="col-md-6">
    <div class="row page-header">
      <h1>Cheap, Fast, Eats!</h1>
    </div>
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>Review Count</th>
          <th>Restaurant</th>
          <th>Rating</th>
          <th>Review</th>
        </tr>
      </thead>
      <tbody>
        <%= @responses.each do |place| %>
        <tr>
          <td><%= place.review_count %></td>
          <td><a href=<%= place.mobile_url %>><%= place.name %></a></td>
          <td><%= place.rating %></td>
          <td><%= place.snippet_text %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

